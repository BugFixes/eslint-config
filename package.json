{
  "name": "@bugfixes/eslint-config",
  "version": "0.0.4",
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "main": "index.js",
  "files": [
    "rules"
  ],
  "repository": {
    "url": "git+https://github.com/bugfixes/eslint-config.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/bugfixes/eslint-config/issues"
  },
  "homepage": "https://bugfix.es",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "setup:hooks": "husky install",
    "test": "npm run lint"
  },
  "keywords": [
    "bugfixes",
    "eslint",
    "eslintconfig"
  ],
  "peerDependencies": {
    "eslint": "^7.32.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "standard-version": "^9.3.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-redux": "^3.3.2",
    "eslint-plugin-tsdoc": "^0.2.14"
  }
}
